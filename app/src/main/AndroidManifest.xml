            android:windowSoftInputMode="stateHidden" />
        <activity android:name="com.guideforwinjo.winzogoldwin.tipswinzo.Activity.llGamesActivity" />
        <activity
            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.Activity.AtmeGamesActivity"
