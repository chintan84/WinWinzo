            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.Activity.GirlsDressActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.Activity.layWinsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.Activity.WebActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode"
            android:screenOrientation="sensor"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.Activity.ActionGamesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.Activity.AdventureGamesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.Activity.imulatorGamesActivity"
