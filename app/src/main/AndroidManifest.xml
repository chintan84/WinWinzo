        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:hardwareAccelerated="true"
        android:theme="@style/AppTheme.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.Activity.DashBoardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.First.FirstActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.Activity.earchGameActivity"
            android:screenOrientation="portrait"
