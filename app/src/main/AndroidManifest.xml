            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.First.TaptoStartActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.First.MoreActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.First.ThankYouActivity"
            android:screenOrientation="portrait" />

