        <activity
            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.Activity.portsGamesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.Activity.GameZopActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.Activity.acingGamesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.facebook.ads.AudienceNetworkActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:exported="false"
            android:hardwareAccelerated="true"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.Guide.Language_Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.guideforwinjo.winzogoldwin.tipswinzo.Guide.GuideMain_Activity"
